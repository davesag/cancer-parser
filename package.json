{
  "name": "cancer-parser",
  "version": "1.0.0",
  "description": "Parses cancer incident data from excel",
  "main": "index.js",
  "files": [
    "/package*.json",
    "/src",
    "/CONTRIBUTING.md"
  ],
  "scripts": {
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "lint": "eslint .",
    "prettier": "prettier --write '**/*.{js,json,md}'",
    "start": "node index.js",
    "test": "npm run test:unit",
    "test:unit": "NODE_ENV=test mocha ./test/unit/ --require ./test/unitTestHelper.js --recursive",
    "test:unit:cov": "NODE_ENV=test nyc mocha ./test/unit/ --require ./test/unitTestHelper.js --recursive",
    "test:mutants": "NODE_ENV=test npx stryker run"
  },
  "keywords": [
    "parser"
  ],
  "author": "Dave Sag <david.sag@industrie.co>",
  "license": "MIT",
  "repository": "https://github.com/industrieco/cancer-parser.git",
  "bugs": {
    "url": "https://github.com/industrieco/cancer-parser/issues"
  },
  "homepage": "https://github.com/industrieco/cancer-parser#README",
  "dependencies": {
    "read-excel-file": "^4.0.3"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^2.1.0",
    "@stryker-mutator/javascript-mutator": "^2.1.0",
    "@stryker-mutator/mocha-framework": "^2.1.0",
    "@stryker-mutator/mocha-runner": "^2.1.0",
    "ajv": "^6.10.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-mocha": "^6.2.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "proxyquire": "^2.1.3",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "proseWrap": "never"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/index.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "all": true,
    "cache": true
  }
}
